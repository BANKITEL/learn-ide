{
  "name": "learn-ide",
  "version": "2.0.1",
  "atomVersion": "1.12.0",
  "treeVersion": "1.0.9",
  "main": "./lib/learn-ide",
  "urlMain": "./lib/url-handler",
  "description": "An integrated development environment for use with Learn.co",
  "scripts": {
    "start": "./node_modules/gulp/bin/gulp.js"
  },
  "keywords": [],
  "repository": "https://github.com/learn-co/learn-ide",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "dependencies": {
    "atom-socket": "0.0.1",
    "atom-space-pen-views": "2.1.0",
    "base-64": "0.1.0",
    "coffee-script": "1.11.1",
    "cross-spawn": "2.1.5",
    "dotenv": "2.0.0",
    "fs-plus": "2.9.1",
    "http-proxy": "1.13.3",
    "learn-term.js": "0.0.7",
    "mkdirp": "0.5.1",
    "page-bus": "3.0.1",
    "rimraf": "2.1",
    "rmdir": "1.2.0",
    "single-socket": "0.0.10",
    "underscore-plus": "1.6.6",
    "utf8": "2.1.1",
    "winston": "2.3.0"
  },
  "configSchema": {
    "terminalFontColor": {
      "type": "string",
      "default": "#ffffff",
      "title": "Terminal Font Color",
      "description": "A valid CSS color value which will be used to style your terminal's font"
    },
    "terminalBackgroundColor": {
      "type": "string",
      "default": "#000000",
      "title": "Terminal Background Color",
      "description": "A valid CSS color value which will be used to style your terminal's background"
    }
  },
  "devDependencies": {
    "decompress": "4.0.0",
    "del": "2.2.2",
    "gulp": "3.9.1",
    "gulp-util": "3.0.7",
    "request": "2.75.0",
    "run-sequence": "1.2.2",
    "shelljs": "0.7.3",
    "ssh2": "0.5.0"
  }
}
